{
  "name": "onr_amp",
  "version": "1.0.0",
  "description": "Custom theme for Office of Naval Research using USWDS library - AMP",
  "author": "Stephen Mustgrave",
  "license": "ISC",
  "private": true,
  "scripts": {
    "init": "npm i --legacy-peer-dep; gulp",
    "build": "gulp",
    "lint": "gulp scss-lint; gulp js-lint",
    "styleguide": "gulp styleguide",
    "imagemin": "gulp imagemin",
    "watch": "gulp watch",
    "scss": "gulp scss"
  },
  "paths": {
    "dist": {
      "css": "./css/",
      "js": "./js/",
      "sg": "./styleguide/",
      "img": "./images/"
    },
    "scss": [
      "./components/**/*.scss"
    ],
    "css": [
      "./*.css"
    ],
    "js": [
      "./components/**/*.js"
    ],
    "templates": [
      "./templates/**/*.twig",
      "./styleguide/*.html"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "cssnano": "^5.0.8",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-load-plugins": "^2.0.7",
    "gulp-pipe": "^1.0.4",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify-es": "^3.0.0",
    "kss": "^3.0.1",
    "michelangelo": "^0.8.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.9",
    "postcss-csso": "^5.0.1",
    "require-dir": "^1.2.0"
  }
}
